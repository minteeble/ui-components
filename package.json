{
  "name": "@minteeble/ui-components-lib",
  "devDependencies": {
    "lerna": "^6.5.1"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "lerna run build --scope=@minteeble/ui-components",
    "build-extra": "lerna run build --scope=@minteeble/ui-components-extra",
    "storybook": "lerna run storybook --scope=@minteeble/ui-components",
